programa
{
	funcao inicio()
	{
		 // Crie um programa que leia o nome e o salário de um funcionário, mostrando no final uma mensagem.

		 cadeia nome
		 real salario

		 escreva ("O nome do funcionario e: ")
		 leia (nome)

		 escreva ("O salario do funcionario e: ")
		 leia (salario)

		 escreva ("O salario de ", nome ," e de ", salario ," reais.")
	}
}
